public class ChemicalElements {
    private String name;
    private String symbol;
    private int number;
    private String type;

    public ChemicalElements() {

    }

    public ChemicalElements(String name, String symbol, int number, String type){
        this.name = name;
        this.symbol = symbol;
        this.number = number;
        this.type = type;

    }

    public String getName() {
        return this.name;
    }

    public String getSymbol(){
        return  this.symbol;
    }

    public int getNumber(){
        return this.number;
    }

    public String type(){
        return this.type;
    }

    public String toString() {
        String s = "";
        s = s + name + " (" + symbol + ")\n";
        s = s + "Atomic number: " + number + "\n";
        s = s + "Type of metal: " + type + "\n";
        return s;
    }

    public static void main(String[] args) {

    }
}
